{% extends 'base.html.twig' %}

{% block title %}Page d'appel - U10

{% endblock %}

{% block body %}

	<div class="col-lg-8 mx-auto p-5 py-md-5">
		<div class="mt-3 text-center">
			<h1 class="h1 mb-2">Page d'appel - U10</h1>
		</div>

		{% set team = 12 %}

		<div class="row row-cols-1 row-cols-md-2 g-4 pt-5">
			{% for tbl_player in tbl_player %}
				{{ tbl_player.lastName }}
			{% endfor %}
			<div class="col">
				<a href="/appel/U{{ team }}">
					<div class="card">
						<img src="/images/U{{ team }}.png" class="zoom" alt="1">
					</div>
				</a>
			</div>
		</div>
	</div>

{% endblock %}

