    {% block body %}
      <div class="container-fluid py-4">
        <div class="row">
          <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div class="icon icon-lg icon-shape bg-gradient-dark shadow-dark text-center border-radius-xl mt-n4 position-absolute">
                  <i class="fa-solid fa-futbol"></i>              
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Nombre de tests</p>
                  <h4 class="mb-0">{{ testcount }}</h4>
                </div>
              </div>
              <hr class="dark horizontal my-0">
              <div class="card-footer p-3">
                <p class="mb-0"><span class="text-success text-sm font-weight-bolder">+{{addedtest}}</span> test(s) ce mois ci</p>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div class="icon icon-lg icon-shape bg-gradient-primary shadow-primary text-center border-radius-xl mt-n4 position-absolute">
                  <i class="fa-brands fa-superpowers"></i>
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Status</p>
                  <h4 class="mb-0">
                      {% if "ROLE_SUPER_ADMIN" in app.user.getRoles() %}
                          Développeur
                      {% elseif "ROLE_COACH" in app.user.getRoles() %}
                          Coach
                      {% elseif "ROLE_USER" in app.user.getRoles() %}
                          Joueur
                      {% else %}
                          ERROR
                      {% endif %}
                  </h4>
                </div>
              </div>
              <hr class="dark horizontal my-0">
              <div class="card-footer p-3">
                <p class="mb-0"><span class="text-success text-sm font-weight-bolder">{{usercount}}</span> comptes sur l'application</p>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-sm-6 mb-xl-0 mb-4">
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div class="icon icon-lg icon-shape bg-gradient-success shadow-success text-center border-radius-xl mt-n4 position-absolute">
                  <i class="fa-solid fa-calendar-days"></i>
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Date du jour</p>
                  {% set today = date() %}
                  <h4 class="mb-0">{{ today|date('d/m/Y') }}</h4>
                  {% set lastDayOfMonth = today|date_modify('last day of this month') %}
                  {% set daysRemaining = lastDayOfMonth|date_modify('-' ~ today|date('j') ~ ' day')|date('j') %}

                </div>
              </div>
              <hr class="dark horizontal my-0">
              <div class="card-footer p-3">
                <p class="mb-0"><span class="text-danger text-sm font-weight-bolder">{{ daysRemaining }}</span> jour(s) restant avec le mois prochain</p>
              </div>
            </div>
          </div>
          <div class="col-xl-3 col-sm-6">
            <div class="card">
              <div class="card-header p-3 pt-2">
                <div class="icon icon-lg icon-shape bg-gradient-info shadow-info text-center border-radius-xl mt-n4 position-absolute">
                  <i class="fa-solid fa-scale-balanced"></i>
                </div>
                <div class="text-end pt-1">
                  <p class="text-sm mb-0 text-capitalize">Prochain enregistrement poid/taille</p>
                  {% set modifiedDate = weightIn|date_modify('+3 months') %}
                  <h4 class="mb-0">{{ modifiedDate|date('d/m/Y') }}</h4>
                </div>
              </div>
              <hr class="dark horizontal my-0">
              <div class="card-footer p-3">
                <p class="mb-0">
                {% set currentDate = "now"|date("Y-m-d") %}
                {% set modifiedDateYmd = modifiedDate|date("Y-m-d") %}
                {% set currentDateYmd = currentDate|date("Y-m-d") %}
                {% set diff = modifiedDateYmd|date_modify("midnight")|date("U") - currentDateYmd|date_modify("midnight")|date("U") %}
                {% set remainingDays = (diff / (24 * 3600))|round %}
                <span class="text-danger text-sm font-weight-bolder">{{ remainingDays }}</span> jours restant
                </p>
              </div>
            </div>
          </div>
        </div>
        <div class="row mt-4"></div>
        <div class="row mb-4">
          <div class="col-lg-8 col-md-6 mb-md-0 mb-4">
            <div class="card">
              <div class="card-header pb-0">
                <div class="row">
                  <div class="col-lg-6 col-7">
                    <h6>Équipe</h6>
                    <p class="text-sm mb-0">
                      <i class="fa-solid fa-users"></i>
                      <span class="font-weight-bold ms-1">{{ equipeusercount }}</span> joueur(s) dans l'équipe
                    </p>
                  </div>  
                  <div class="col-lg-6 col-5 my-auto text-end">
                    <div class="dropdown float-lg-end pe-4">
                      <a class="cursor-pointer" id="dropdownTable" data-bs-toggle="dropdown" aria-expanded="false">
                        <i class="fa fa-ellipsis-v text-secondary"></i>
                      </a>
                      <ul class="dropdown-menu px-2 py-3 ms-sm-n4 ms-n5" aria-labelledby="dropdownTable">
                        <li><a class="dropdown-item border-radius-md" href="javascript:;">Action</a></li>
                        <li><a class="dropdown-item border-radius-md" href="javascript:;">Another action</a></li>
                        <li><a class="dropdown-item border-radius-md" href="javascript:;">Something else here</a></li>
                      </ul>
                    </div>
                  </div>
                </div>
              </div>
              <div class="card-body px-0 pb-2">
                <div class="table-responsive">
                  <table class="table align-items-center mb-0">
                    <thead>
                      <tr>
                        <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Nom</th>
                        <th class="text-uppercase text-secondary text-xxs font-weight-bolder opacity-7 ps-2">Role</th>
                        <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Poste principal</th>
                        <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Poste secondaire</th>
                        <th class="text-center text-uppercase text-secondary text-xxs font-weight-bolder opacity-7">Date Naissance</th>
                      </tr>
                    </thead>
                    <tbody>

                    {% for player in equipeuser %}
                      <tr>
                        <td>
                          <div class="d-flex px-2 py-1">
                            <div>
                              <img src="/uploads/images/{{ player.getProfileImage() }}" class="avatar avatar-sm me-3" alt="Profile Image">
                            </div>
                            <div class="d-flex flex-column justify-content-center">
                              <h6 class="mb-0 text-sm">{{ player.firstname }} {{ player.lastname }}</h6>
                            </div>
                          </div>
                        </td>
                        {#<td>
                          <div class="avatar-group mt-2">
                            <a href="javascript:;" class="avatar avatar-xs rounded-circle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Ryan Tompson">
                              <img src="/assets/img/team-1.jpg" alt="team1">
                            </a>
                            <a href="javascript:;" class="avatar avatar-xs rounded-circle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Romina Hadid">
                              <img src="/assets/img/team-2.jpg" alt="team2">
                            </a>
                            <a href="javascript:;" class="avatar avatar-xs rounded-circle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Alexander Smith">
                              <img src="/assets/img/team-3.jpg" alt="team3">
                            </a>
                            <a href="javascript:;" class="avatar avatar-xs rounded-circle" data-bs-toggle="tooltip" data-bs-placement="bottom" title="Jessica Doe">
                              <img src="/assets/img/team-4.jpg" alt="team4">
                            </a>
                          </div> 
                        </td>#}
                        <td class="align-middle text-left text-sm">
                          <span class="text-xs font-weight-bold">{{ "ROLE_SUPER_ADMIN" in player.roles ? 'Développeur' : ("ROLE_COACH" in app.user.getRoles() ? 'Coach' : ("ROLE_USER" in app.user.getRoles() ? 'Joueur' : 'ERROR')) }}</span>
                        </td>
                        <td class="align-middle text-center text-sm">
                          <span class="text-xs font-weight-bold">{% if player.getPostePrincipal() != null %} {{ player.getPostePrincipal() }} {% else %} - {% endif %}</span>
                        </td>
                        <td class="align-middle text-center text-sm">
                          <span class="text-xs font-weight-bold">{% if player.getPosteSecondaire() != null %} {{ player.getPosteSecondaire() }} {% else %} - {% endif %}</span>
                        </td>
                        <td class="align-middle text-center text-sm">
                          <span class="text-xs font-weight-bold">{{ player.getDateNaissance()|date('d/m/Y') }}</span>
                        </td>
                        {# <td class="align-middle">
                          <div class="progress-wrapper w-75 mx-auto">
                            <div class="progress-info">
                              <div class="progress-percentage">
                                <span class="text-xs font-weight-bold">60%</span>
                              </div>
                            </div>
                            <div class="progress">
                              <div class="progress-bar bg-gradient-info w-60" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100"></div>
                            </div>
                          </div>
                        </td> #}
                      </tr>
                    {% endfor %}
                    </tbody>
                  </table>
                </div>
              </div>
            </div>
          </div>
          <div class="col-lg-4 col-md-6">
            <div class="card h-100">
              <div class="card-header pb-0">
                <h6>Paliers</h6>
                <p class="text-sm">
                  <i class="fa fa-arrow-up text-success" aria-hidden="true"></i>
                  Palier actuel :<span class="font-weight-bold"> Palier {{ app.user.getPalier().numero }}</span>
                </p>
              </div>
              <div class="card-body p-3">
                <div class="timeline timeline-one-side">
                  {% for palier in paliers %}
                    <div class="timeline-block mb-3">
                      <span class="timeline-step">
                        {% set currentPalierNumber = app.user.getPalier().getNumero() %}

                        {% if palier.getNumero() < currentPalierNumber %}
                            <i class="fa-solid fa-check text-success"></i>
                        {% elseif palier.getNumero() > currentPalierNumber %}
                          <i class="fa-solid fa-lock text-danger"></i> 
                        {% else %}
                          <i class="fa-solid fa-hourglass-half text-warning"></i>
                        {% endif %}                      
                      </span>
                      <div class="timeline-content">
                        <h6 class="text-dark text-sm font-weight-bold mb-0">Palier {{palier.numero}} - {{ palier.name }}</h6>
                        <p class="text-secondary font-weight-bold text-xs mt-1 mb-0">{{ palier.objectif }}</p>
                      </div>
                    </div>
                  {% endfor %}
                </div>
              </div>
            </div>
          </div>
        </div>
        <footer class="footer py-4  ">
          <div class="container-fluid">
            <div class="row align-items-center justify-content-lg-between">
              <div class="col-lg-6 mb-lg-0 mb-4">
                <div class="copyright text-center text-sm text-muted text-lg-start">
                  Créé par les BTS SIO 2 NDLP (2023/2024).
                </div>
              </div>
              
            </div>
          </div>
        </footer>
      </div>
    {% endblock %}
